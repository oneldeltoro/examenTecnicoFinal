drop table if exists waste_center_authorization_entity cascade
drop table if exists waste_manager_address_entity cascade
drop table if exists waste_manager_entity cascade
create table waste_center_authorization_entity (created_date date, last_modified_date date, id bigint generated by default as identity, waste_manager_entity_id bigint, auth_number varchar(255), primary key (id))
create table waste_manager_address_entity (created_date date, is_enabled boolean, last_modified_date date, id bigint generated by default as identity, version bigint, direccion varchar(255) not null, primary key (id))
create table waste_manager_entity (created_date date, is_enabled boolean, last_modified_date date, address_entity_id bigint not null, id bigint generated by default as identity, version bigint, nif varchar(255) not null, nombre varchar(255) not null, primary key (id))
alter table if exists waste_center_authorization_entity add constraint FKk9e9kq9a71o2bue0ix0gwcsaq foreign key (waste_manager_entity_id) references waste_manager_entity
alter table if exists waste_manager_entity add constraint FKirssvai05j6n36p3etgugfrcj foreign key (address_entity_id) references waste_manager_address_entity
